{
  "name": "kunalnagar.in",
  "version": "1.3.7",
  "description": "Personal website",
  "homepage": "https://www.kunalnagar.in",
  "dependencies": {
    "bootstrap": "^4.5.0",
    "bootstrap-scss": "^4.3.1",
    "husky": "^4.2.5",
    "normalize.scss": "^0.1.0",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1"
  },
  "devDependencies": {
    "snyk": "^1.369.2"
  },
  "scripts": {
    "start": "docker-compose up -d",
    "stop": "docker-compose down",
